<%= simple_form_for @proposal do |f| %>
  <%= f.input :title %>
  <%= f.input :tagline %>
  <%= f.input :status, collection: ["Draft", "Pending", "Accetped", "Rejected"], prompt: "Choose a Status" %>
  <%= f.association :project, collection: Project.order(:title), prompt: "Choose a Project" %>
  <%= f.button :submit %>
<% end %>
